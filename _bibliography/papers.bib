   author = {Wang, F. and Luo, Q. and Chen, Y. and Liu, Y. and Xu, K. and Adhikari, K. and Cai, X. and Liu, J. and Li, Y. and Liu, X. and Ramirez-Aristeguieta, L. M. and Yuan, Z. and Zhou, Y. and Li, F. F. and Jiang, B. and Jin, L. and Ruiz-Linares, A. and Yang, Z. and Liu, F. and Wang, S.},
